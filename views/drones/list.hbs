<h2>Available drones</h2>
{{#each drones}}
<h3>Name: {{this.name}}</h3>
<h3>Propellers: {{this.propellers}}</h3>
<h3>MaxSpeed {{this.maxSpeed}}</h3>
<button>
<a href="/drones/{{id}}/edit">Edit</a>
</button>
<form action="/drones/{{id}}/delete" method="post">
<button>Delete</button>
</form>
{{/each}}
